info='GNU autoconf, a tool for generating configure scripts'
version=2.69
source=http://ftp.gnu.org/gnu/$name/$name-$version.tar.xz
deps='m4 perl'
sha256=64ebcec9f8ac5b2487125a86a7760d2591ac9e1d3dbd59489633f9de62a57684
build() {
    # autoconf doesn't understand -musl triplet
    unset BUILD HOST TARGET

    bonsai_configure
    bonsai_make
    bonsai_make install
}
