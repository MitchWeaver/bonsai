info='Portable uuid C library'
version=1.0.3
source=http://downloads.sourceforge.net/sourceforge/$name/$version/$name-$version.tar.gz
sha256=46af3275291091009ad7f1b899de3d0cea0252737550e7919d17237997db5644
deps=libtool

build() {
    bonsai_configure
    cp /bin/libtool .
    bonsai_make
    bonsai_make install
}
