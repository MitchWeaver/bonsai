info='X11 Miscellaneous Utilties'
version=1.1.3
source=http://x.org/releases/individual/lib/libXmu-$version.tar.bz2
deps='xorg-libxt xorg-libxext xorg-util-macros'
sha256=9c343225e7c3dc0904f2122b562278da5fed639b1b5e880d25111561bac5b731
build() {
    bonsai_configure \
        --disable-lint-library \
        --without-lint
    bonsai_make
    bonsai_make install
}
